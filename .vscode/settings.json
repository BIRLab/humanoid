{
    "C_Cpp.intelliSenseEngine": "disabled",
    "clangd.onConfigChanged": "restart",
    "C_Cpp.clang_format_fallbackStyle": "Google",
    "clangd.arguments": [
        "-header-insertion=never"
    ],
    "files.eol": "\n",
    "ros.distro": "foxy",
    "ros.rosSetupScript": "${workspaceFolder}/install/setup.bash",
    "ros.isolateEnvironment": "true",
    "python.analysis.extraPaths": [
        "/opt/ros/foxy/lib/python3.8/site-packages",
        "${workspaceFolder}/build/humanoid_web",
        "${workspaceFolder}/install/humanoid_web/lib/python3.8/site-packages",
        "${workspaceFolder}/build/humanoid_arm",
        "${workspaceFolder}/install/humanoid_arm/lib/python3.8/site-packages",
        "${workspaceFolder}/build/humanoid_interface",
        "${workspaceFolder}/install/humanoid_interface/lib/python3.8/site-packages",
    ],
    "python.defaultInterpreterPath": "/bin/python3",
    "clangd.path": "clangd-17"
}